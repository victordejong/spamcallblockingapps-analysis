<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.5dip" />
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff9d9e9d" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff9d9e9d" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="1.5dip" />
                <gradient android:startColor="#ff0084ff" android:endColor="#ff0084ff" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0084ff" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.5dip" />
                <gradient android:startColor="#ff0084ff" android:endColor="#ff0084ff" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0084ff" />
            </shape>
        </clip>
    </item>
</layer-list>