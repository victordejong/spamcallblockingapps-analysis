    <item android:state_selected="false" android:id="@id/un_selected" android:drawable="@drawable/flash_ic_location__default_24dp" />
    <transition android:drawable="@drawable/avd_location_un_selected_selected" android:toId="@id/selected" android:fromId="@id/un_selected" android:reversible="true" />
</animated-selector>