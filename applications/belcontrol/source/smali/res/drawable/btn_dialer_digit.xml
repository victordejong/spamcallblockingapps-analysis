<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/btn_dialer_bg_pressed" />
            <stroke android:width="1.0dip" android:color="@color/image_bg" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white_fontcolor" />
            <stroke android:width="1.0dip" android:color="@color/white_fontcolor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>